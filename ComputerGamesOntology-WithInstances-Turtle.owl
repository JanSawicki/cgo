<http://www.semanticweb.org/jansawicki/ontologies/cgo> rdf:type owl:Ontology ;
                                                        owl:versionIRI <http://www.semanticweb.org/jansawicki/ontologies/cgo/1.0.0> ;
                                                        dc:created "2019-11-14T13:52:03"^^xsd:dateTime ,
                                                                   "Jan Sawicki"^^xsd:string ;
                                                        dc:dateAccepted "2020-01-28T20:00:00"^^xsd:dateTime ;
                                                        dc:dateSubmitted "2020-01-21T16:00:00"^^xsd:dateTime ;
                                                        dc:description "The Computer Games Ontology is dedicated to solving the problem of finding games by their similarities. That means it is created to serve anyone who looks for a game and knows only their partial or full requirement but not the title itself."^^xsd:string ;
