###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Abbreviation
cgo:Abbreviation rdf:type owl:Class ;
                 rdfs:comment "An abberviation has to be an abbreviation of something. Otherwise it does not exist." ,
                              "An abbreviation of a name." .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Action
cgo:Action rdf:type owl:Class ;
           rdfs:subClassOf cgo:Genre ;
           rdfs:comment "e.g. first person shooters, horrors" .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#ActionTime
cgo:ActionTime rdf:type owl:Class ;
               rdfs:comment "ActionTime can be attributed to both a game (directly) or to a particular lore." ,
                            "The time period where the action of the game takes place." .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Actor
cgo:Actor rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( cgo:Person
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty cgo:isActorOf ;
                                                       owl:someValuesFrom cgo:Movie
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
          rdfs:subClassOf cgo:Person ;
          rdfs:comment cgo:Person ,
                       "A person that stars in movies." .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#AdvancedLore
cgo:AdvancedLore rdf:type owl:Class ;
                 owl:equivalentClass [ owl:intersectionOf ( cgo:Lore
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty cgo:hasArtPiece ;
                                                              owl:minQualifiedCardinality "5"^^xsd:nonNegativeInteger ;
                                                              owl:onClass cgo:ArtPiece
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                     ] ;
                 rdfs:subClassOf cgo:Lore ;
                 rdfs:comment "A subjective definition of lore that is advanced." ,
                              "An AdvancedLore has minimum 5 related ArtPieces." ;
                 rdfs:seeAlso cgo:SimpleLore .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Adventure
cgo:Adventure rdf:type owl:Class ;
              rdfs:subClassOf cgo:Genre ;
              rdfs:comment "e.g. text adventures" .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Android
cgo:Android rdf:type owl:Class ;
            rdfs:subClassOf cgo:MobileOperatingSystem ;
            rdfs:comment "A distribution of Android (mobile operating system)." .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#ArtPiece
cgo:ArtPiece rdf:type owl:Class ;
             rdfs:comment "A piece of art that may somehow be related to a game." ,
                          "Piece of art may not have an Author (e.g. Author is unknown)." ,
                          "PieceOfArt may have many authors (e.g. Movies)" .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#AudioDevice
cgo:AudioDevice rdf:type owl:Class ;
                rdfs:subClassOf cgo:InputDevice ,
                                cgo:OutputDevice ;
                rdfs:comment "A device outputing audio." ,
                             "E.g. headphones, speakers." .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Author
cgo:Author rdf:type owl:Class ;
           owl:equivalentClass [ owl:intersectionOf ( cgo:Person
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty cgo:isRelatedTo ;
                                                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                        owl:onClass cgo:ArtPiece
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] ;
           rdfs:subClassOf cgo:Person ;
           rdfs:comment "An author of something." .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#CPU
cgo:CPU rdf:type owl:Class ;
        rdfs:subClassOf cgo:Console ,
                        cgo:Hardware ;
        rdfs:comment "A central processing unit." .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Company
cgo:Company rdf:type owl:Class ;
            rdfs:comment "A Company may be related (exist) in many countries." ,
                         "A company related to games." .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#ComputerGame
cgo:ComputerGame rdf:type owl:Class ;
                 rdfs:subClassOf cgo:Game ;
                 rdfs:comment "A Game that is distributed for a Computer." ,
                              """Determining wheather a Game is a ComputerGame depends if it runs on an OperatingSystem which is a ComputerOperatingSystem. 
This logic is different for ConsoleGame.""" ;
                 rdfs:seeAlso cgo:ComputerOperatingSystem ,
                              cgo:ConsoleGame ,
                              cgo:Game .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#ComputerOperatingSystem
cgo:ComputerOperatingSystem rdf:type owl:Class ;
                            rdfs:subClassOf cgo:OperatingSystem .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Console
cgo:Console rdf:type owl:Class ;
            rdfs:subClassOf cgo:Platform ;
            rdfs:comment "A console dedicated to gaming." ,
                         "E.g. PlayStation, XBox." .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#ConsoleGame
cgo:ConsoleGame rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( cgo:Game
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty cgo:hasPlatform ;
                                                             owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                             owl:onClass cgo:Console
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] ;
                rdfs:subClassOf cgo:Game ;
                rdfs:comment "A Game that is distributed for a Console." ,
                             """Determining wheather a Game is a ConsoleGame depends if it has a Platform which is a Console.
This logic is different for ComputerGame and MobileGame.""" ;
                rdfs:seeAlso cgo:ComputerGame ,
                             cgo:Console ,
                             cgo:Game ,
                             cgo:MobileGame .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Convention
cgo:Convention rdf:type owl:Class ;
               rdfs:subClassOf cgo:Event ;
               rdfs:comment "A convention related to a Game." ;
               rdfs:seeAlso cgo:Game .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Country
cgo:Country rdf:type owl:Class ;
            rdfs:comment "A country." ,
                         """This could be replaced with an object property. However, to avoid confusion, it is advised to used a class.

\"Confusion\", e.g. \"Germany\" vs \"Federal Republic of Germany\".""" .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Developer
cgo:Developer rdf:type owl:Class ;
              owl:equivalentClass [ owl:intersectionOf ( cgo:Company
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty cgo:isDeveloperOf ;
                                                           owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                           owl:onClass cgo:Game
                                                         ]
                                                       ) ;
                                    rdf:type owl:Class
                                  ] ;
              rdfs:subClassOf cgo:Company ;
              rdfs:comment "A company that is creating (developing) games." ;
              rdfs:seeAlso cgo:Company .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Difficulty
cgo:Difficulty rdf:type owl:Class ;
               rdfs:comment "A Game difficulty." ,
                            "When adding Difficulty instances note that they should have explanations and Game references (e.g.: \"difficult\" as Dark Souls, \"easy\" as Candy Crash)." ;
               rdfs:seeAlso cgo:Game .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Director
cgo:Director rdf:type owl:Class ;
             owl:equivalentClass [ owl:intersectionOf ( cgo:Person
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty cgo:isDirectorOf ;
                                                          owl:someValuesFrom cgo:Movie
                                                        ]
                                                      ) ;
                                   rdf:type owl:Class
                                 ] ;
             rdfs:subClassOf cgo:Person ;
             rdfs:comment cgo:Person ,
                          "A person who directs movies." .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Distributor
cgo:Distributor rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( cgo:Company
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty cgo:isDistributorOf ;
                                                             owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                             owl:onClass cgo:Game
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] ;
                rdfs:subClassOf cgo:Company ;
                rdfs:comment "A company that distributes (sells) games." ;
                rdfs:seeAlso cgo:Company .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Driver
cgo:Driver rdf:type owl:Class ;
           rdfs:subClassOf cgo:Software ;
           rdfs:comment "A computer driver." .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Educational
cgo:Educational rdf:type owl:Class ;
                rdfs:subClassOf cgo:Genre ;
                rdfs:seeAlso "e.g. discovery games, learning games" .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#EsportGame
cgo:EsportGame rdf:type owl:Class ;
               owl:equivalentClass [ owl:intersectionOf ( cgo:Game
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty cgo:hasEvent ;
                                                            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                            owl:onClass cgo:Tournament
                                                          ]
                                                        ) ;
                                     rdf:type owl:Class
                                   ] ;
               rdfs:subClassOf cgo:Game ;
               rdfs:comment "A Game that has at least one tournament/championship/other competetive event." ;
               rdfs:seeAlso cgo:Game ,
                            cgo:Tournament .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Esporter
cgo:Esporter rdf:type owl:Class ;
             owl:equivalentClass [ owl:intersectionOf ( cgo:Person
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty cgo:isEsporterOf ;
                                                          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                          owl:onClass cgo:Tournament
                                                        ]
                                                      ) ;
                                   rdf:type owl:Class
                                 ] ;
             rdfs:subClassOf cgo:Person ;
             rdfs:comment "A Person playing EsportGames (i,e, playing Games competetively)." ,
                          "It is enough for a person to play one EsportGame (i.e. play in a single Tournament) to be a Esporter." ,
                          "This class may also be called \"Player\", but it would be too confusing, becuase everyone that plays games can be called a \"player\"." .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Event
cgo:Event rdf:type owl:Class ;
          rdfs:comment "An event of any kind." .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#FirstPerson
cgo:FirstPerson rdf:type owl:Class ;
                rdfs:subClassOf cgo:Genre ;
                rdfs:comment "Describes Games which are played from the first person perspective." ,
                             "e.g. first person shooter" ;
                rdfs:seeAlso cgo:ThirdPerson .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#GPU
cgo:GPU rdf:type owl:Class ;
        rdfs:subClassOf cgo:Console ,
                        cgo:Hardware ;
        rdfs:comment "A graphics processing unit." .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Game
cgo:Game rdf:type owl:Class ;
         owl:equivalentClass cgo:VideoGame ;
         rdfs:comment "A Game has to have a distinct name." ,
                      "A Game may have many Platforms (e.g. it can be distributed for computers and consoles)." ,
                      "A Game should be treated as a title, not a computer program." ,
                      "A game can have multiple developers." ,
                      "A game can have multiple distributors." ,
                      "A game can have multiple publishers." ,
                      """A game comes to life in three steps:
- development
- publishing
- distribution""" ,
                      "A game may have multiple Universes (cross-overs etc.)" ,
                      "The central point of the whole ontology." .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#GameSeries
cgo:GameSeries rdf:type owl:Class ;
               rdfs:comment "A series of Games, which consists of at least two Games that are related (represent two Game titles)." .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Genre
cgo:Genre rdf:type owl:Class ;
          rdfs:comment "A genre that can be attributed to a game." ,
                       "All genere super groups are not disjoined because a Game can be of many generes." .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Hardware
cgo:Hardware rdf:type owl:Class ;
             rdfs:comment "A physical device." ,
                          "None of the Hardware subclasses are disjoined because there exists integrated device that are, for example, both input and output." .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#IOS
cgo:IOS rdf:type owl:Class ;
        rdfs:subClassOf cgo:MobileOperatingSystem ;
        rdfs:comment "A distribution of IOS (mobile operating system)." .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Influencer
cgo:Influencer rdf:type owl:Class ;
               owl:equivalentClass [ owl:intersectionOf ( cgo:Person
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty cgo:isInfluencerOf ;
                                                            owl:someValuesFrom cgo:SocialMediaChannel
                                                          ]
                                                        ) ;
                                     rdf:type owl:Class
                                   ] ;
               rdfs:subClassOf cgo:Person ;
               rdfs:comment cgo:Person ,
                            "A person using some social media channel to provide content for their followers." .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#InputDevice
cgo:InputDevice rdf:type owl:Class ;
                rdfs:subClassOf cgo:Hardware ;
                rdfs:comment "A hardware device which inputs some information." .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Language
cgo:Language rdf:type owl:Class ;
             rdfs:comment "A languge that a game is provided in." ;
             rdfs:seeAlso """This could be replaced with an object property. However, to avoid confusion, it is advised to used a class.

\"Confusion\", e.g. \"english\" vs \"English\" vs \"american english\".""" .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Launcher
cgo:Launcher rdf:type owl:Class ;
             rdfs:subClassOf cgo:Software ;
             rdfs:comment "A launcher/client used to run a game."^^xsd:string ,
                          "e.g. Steam, Origin"^^xsd:string .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Lore
cgo:Lore rdf:type owl:Class ;
         rdfs:comment "A backstory created around a fictional world." .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Macintosh
cgo:Macintosh rdf:type owl:Class ;
              rdfs:subClassOf cgo:ComputerOperatingSystem ;
              rdfs:comment "A distribution of Macintosh Operating System (macOS)." .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Meme
cgo:Meme rdf:type owl:Class ;
         rdfs:subClassOf cgo:ArtPiece ;
         rdfs:comment "A meme has no creator nor author. It is a common wealth." ,
                      "An element of a culture passed from one individual to another with digial means." ;
         rdfs:seeAlso cgo:ArtPiece .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Mobile
cgo:Mobile rdf:type owl:Class ;
           rdfs:subClassOf cgo:Platform .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#MobileGame
cgo:MobileGame rdf:type owl:Class ;
               rdfs:subClassOf cgo:Game ;
               rdfs:comment "A Game that is distributed for mobile platform." ,
                            """Determining wheather a Game is a ComputerGame depends if it runs on an OperatingSystem which is a MobileOperatingSystem. 
This logic is different for ConsoleGame.""" ;
               rdfs:seeAlso cgo:ConsoleGame ,
                            cgo:Game ,
                            cgo:MobileOperatingSystem .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#MobileOperatingSystem
cgo:MobileOperatingSystem rdf:type owl:Class ;
                          rdfs:subClassOf cgo:OperatingSystem .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Mod
cgo:Mod rdf:type owl:Class ;
        rdfs:comment "A modification for a Game." ;
        rdfs:seeAlso cgo:Game .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Movie
cgo:Movie rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( cgo:ArtPiece
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty cgo:hasActor ;
                                                       owl:someValuesFrom cgo:Actor
                                                     ]
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty cgo:hasDirector ;
                                                       owl:someValuesFrom cgo:Director
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
          rdfs:subClassOf cgo:ArtPiece ;
          rdfs:comment "A movie (may also mean e.g. a short film, a cartoon, a TV series)." ,
                       "A movie can also be understood as its script (text)." ;
          rdfs:seeAlso cgo:ArtPiece .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Music
cgo:Music rdf:type owl:Class ;
          rdfs:subClassOf cgo:ArtPiece ;
          rdfs:comment "A piece of music (e.g. a song)." .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Musical
cgo:Musical rdf:type owl:Class ;
            rdfs:subClassOf cgo:Genre ;
            rdfs:seeAlso "e.g. singing games, instrument playing games" .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Musician
cgo:Musician rdf:type owl:Class ;
             owl:equivalentClass [ owl:intersectionOf ( cgo:Person
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty cgo:hasArtPiece ;
                                                          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                          owl:onClass cgo:Music
                                                        ]
                                                      ) ;
                                   rdf:type owl:Class
                                 ] ;
             rdfs:subClassOf cgo:Person .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Name
cgo:Name rdf:type owl:Class ;
         rdfs:comment "A name may have many abbreviations." ,
                      "A name of a thing." ,
                      "Any owl:Thing may have a name." .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#OperatingSystem
cgo:OperatingSystem rdf:type owl:Class ;
                    rdfs:subClassOf cgo:Software ;
                    rdfs:comment "An Operating System run on a computer." .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#OutputDevice
cgo:OutputDevice rdf:type owl:Class ;
                 rdfs:subClassOf cgo:Hardware ;
                 rdfs:comment "A hardware devices which outputs some information." .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#PaymentModel
cgo:PaymentModel rdf:type owl:Class ;
                 rdfs:comment "A game may have many payment models." ,
                              "A way of paying for the game content." ,
                              "e.g. micro transaction, freemium, subscription" .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Person
cgo:Person rdf:type owl:Class ;
           rdfs:comment "A single real person." .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Platform
cgo:Platform rdf:type owl:Class ;
             rdfs:subClassOf cgo:Hardware ;
             rdfs:comment "A game may be available platforms." ,
                          "A platform on which the game may be played." .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Producer
cgo:Producer rdf:type owl:Class ;
             owl:equivalentClass [ owl:intersectionOf ( cgo:Company
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty cgo:isProducerOf ;
                                                          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                          owl:onClass cgo:Game
                                                        ]
                                                      ) ;
                                   rdf:type owl:Class
                                 ] ;
             rdfs:subClassOf cgo:Company ;
             rdfs:comment "A company that produces (pays for the development of) games." ;
             rdfs:seeAlso cgo:Company .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Publisher
cgo:Publisher rdf:type owl:Class ;
              owl:equivalentClass [ owl:intersectionOf ( cgo:Company
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty cgo:isPublisherOf ;
                                                           owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                           owl:onClass cgo:Game
                                                         ]
                                                       ) ;
                                    rdf:type owl:Class
                                  ] ;
              rdfs:subClassOf cgo:Company ;
              rdfs:comment "A company that publishes (prepares to sell) games." ;
              rdfs:seeAlso cgo:Company .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Puzzle
cgo:Puzzle rdf:type owl:Class ;
           rdfs:subClassOf cgo:Genre ;
           rdfs:comment "e.g. mystery solving, quizzes, puzzles" .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#RPG
cgo:RPG rdf:type owl:Class ;
        rdfs:subClassOf cgo:Genre ;
        rdfs:comment "e.g. massive multiplayer online role playing games, roguelikes" .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Racing
cgo:Racing rdf:type owl:Class ;
           rdfs:subClassOf cgo:Genre ;
           rdfs:comment "Note that Fighting Games may or may not be Sport Games." ;
           rdfs:seeAlso cgo:Sport ,
                        "e.g. car racing, plane racing, fantasy racing" .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Rating
cgo:Rating rdf:type owl:Class ;
           rdfs:comment "A rating that the game may have." .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#RatingSystem
cgo:RatingSystem rdf:type owl:Class ;
                 rdfs:comment "A rating system (usually related to a country/set of countries) that defines particulart ratings." .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#RealTime
cgo:RealTime rdf:type owl:Class ;
             rdfs:subClassOf cgo:Strategy ;
             rdfs:comment "Describes Games that have a gameplay in real time (i.e. not turn based)." ;
             rdfs:seeAlso cgo:TurnBased .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Resource
cgo:Resource rdf:type owl:Class ;
             rdfs:comment "A resource or database with information about a game." ,
                          "e.g. internet forum, website, encyclopedia." .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Review
cgo:Review rdf:type owl:Class ;
           rdfs:comment "A Review of a Game." ,
                        "There is no class nor proeprty that maps a review to a particular numeric value, because various sources may have different scales with different precisions and comparing them may be hard." .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#SimpleLore
cgo:SimpleLore rdf:type owl:Class ;
               owl:equivalentClass [ owl:intersectionOf ( cgo:Lore
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty cgo:hasArtPiece ;
                                                            owl:maxQualifiedCardinality "4"^^xsd:nonNegativeInteger ;
                                                            owl:onClass cgo:ArtPiece
                                                          ]
                                                        ) ;
                                     rdf:type owl:Class
                                   ] ;
               rdfs:subClassOf cgo:Lore ;
               rdfs:comment "A subjective definition of lore that is simple." ,
                            "An SimpleLore has maximum 4 related ArtPieces." ;
               rdfs:seeAlso cgo:AdvancedLore .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Simulation
cgo:Simulation rdf:type owl:Class ;
               rdfs:subClassOf cgo:Genre ;
               rdfs:comment "e.g. driving simulations, farming simulations" .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#SizeClassification
cgo:SizeClassification rdf:type owl:Class ;
                       rdfs:comment "The classification of game size." ,
                                    "e.g.: AAA, AAA+, indie" .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#SocialMediaChannel
cgo:SocialMediaChannel rdf:type owl:Class ;
                       owl:equivalentClass [ owl:intersectionOf ( cgo:ArtPiece
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty cgo:hasInfuencer ;
                                                                    owl:someValuesFrom cgo:Influencer
                                                                  ]
                                                                ) ;
                                             rdf:type owl:Class
                                           ] ;
                       rdfs:subClassOf cgo:ArtPiece ;
                       rdfs:comment "A portal for conveying information to massive audiences (usually via the Internet)." ,
                                    "e.g. Twitch, YouTube, Instagram, Facebook" ;
                       rdfs:seeAlso cgo:ArtPiece .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Software
cgo:Software rdf:type owl:Class ;
             rdfs:comment "A computer programs." ;
             rdfs:seeAlso cgo:Game ,
                          "A Game is not a software." .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#SoundCard
cgo:SoundCard rdf:type owl:Class ;
              rdfs:subClassOf cgo:Hardware ;
              rdfs:comment "A sound card used in a computer." .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Sport
cgo:Sport rdf:type owl:Class ;
          rdfs:subClassOf cgo:Genre ;
          rdfs:comment "e.g. football, soccer, basketball" .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Strategy
cgo:Strategy rdf:type owl:Class ;
             rdfs:subClassOf cgo:Genre ;
             rdfs:comment "e.g. card games, real time strategies" .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#SubCompany
cgo:SubCompany rdf:type owl:Class ;
               owl:equivalentClass [ owl:intersectionOf ( cgo:Company
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty cgo:isSubCompanyOf ;
                                                            owl:someValuesFrom cgo:Company
                                                          ]
                                                        ) ;
                                     rdf:type owl:Class
                                   ] ;
               rdfs:subClassOf cgo:Company ;
               rdfs:comment "A company belonging to another company." ;
               rdfs:seeAlso cgo:Company .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#Text
cgo:Text rdf:type owl:Class ;
         owl:equivalentClass [ owl:intersectionOf ( cgo:ArtPiece
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty cgo:hasWriter ;
                                                      owl:someValuesFrom cgo:Writer
                                                    ]
                                                  ) ;
                               rdf:type owl:Class
                             ] ;
         rdfs:subClassOf cgo:ArtPiece ;
         rdfs:comment "A text." ,
                      "e.g. a book or a poem" ;
         rdfs:seeAlso cgo:ArtPiece .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#ThirdPerson
cgo:ThirdPerson rdf:type owl:Class ;
                rdfs:subClassOf cgo:Genre ;
                rdfs:comment "Describes Games which are played from the third person perspective." ,
                             "e.g. third person shooter" ;
                rdfs:seeAlso cgo:FirstPerson .


###  http://www.semanticweb.org/jansawicki/ontologies/cgo#TimeSpan
cgo:TimeSpan rdf:type owl:Class ;
             rdfs:comment "A subjective name given to a real time period when a game was created." ,
                          "The class does not define time precisely (e.g. it can be \"the 90s\" which may mean 1990-2000, but may also include 2001)." ,
